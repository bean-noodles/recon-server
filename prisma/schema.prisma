generator client {
  provider        = "prisma-client"
  output          = "../src/generated/prisma"
  moduleFormat    = "cjs"
}

datasource db {
  provider = "postgresql"
}
        
model User {
  id       String   @id @unique
  order    Int      @default(autoincrement())
  name     String
  email    String   @unique
  picture  String
  createAt DateTime @default(now())
  sites    Site[]
}

model Site {
  id String @id @default(uuid())
  degree Degree
  reason String[]
  clientIp String
  requestTime DateTime
  responseTime DateTime @default(now())
  requestObject Json // 1-a. 검색결과 객체
  
  userId String?
  user User? @relation(fields: [userId], references: [id])
}

enum Degree {
  safe
  caution
  danger
}

